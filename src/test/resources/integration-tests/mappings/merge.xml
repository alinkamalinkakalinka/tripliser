<?xml version="1.0" encoding="UTF-8"?>
<rdf-mapping>
	<namespaces>
		<namespace prefix="food" url="http://bbc.co.uk/food/"/>
		<namespace prefix="foaf" url="http://xmlns.com/foaf/0.1/" />
	</namespaces>
	<graphs>
		<graph name="fruit" query="//food/fruit">
			<resource query="apple">
				<about prepend="http://bbc.co.uk/food/" query="id" />
				<properties>
					<property name="rdf:type" resource="true" value="food:Apple" />
					<property name="foaf:name" query="name" />
				</properties>
			</resource>
		</graph>
		<graph name="vegetables" query="//food/vegetables">
			<resource query="potato">
				<about prepend="http://bbc.co.uk/food/" query="id" />
				<properties>
					<property name="rdf:type" resource="true" value="food:Potato" />
					<property name="foaf:name" query="name" />
				</properties>
			</resource>
			<resource query="cabbage">
				<about prepend="http://bbc.co.uk/food/" query="id" />
				<properties>
					<property name="rdf:type" resource="true" value="food:Cabbage" />
					<property name="foaf:name" query="name" />
				</properties>
			</resource>
		</graph>
	</graphs>
</rdf-mapping>
