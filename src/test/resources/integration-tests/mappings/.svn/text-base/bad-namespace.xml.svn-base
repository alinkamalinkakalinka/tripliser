<?xml version="1.0" encoding="UTF-8"?>
<rdf-mapping>
	<namespaces>
		<namespace prefix="simple" url="^invalid" default="true"/>
	</namespaces>
	<graph>
		<resource query="//simple">
			<about prepend="http://bbc.co.uk/thing/" query="id" />
			<properties>
				<property name="rdf:type" resource="true" value="simple:Thing" />
				<property name="foaf:name" query="name" />
			</properties>
		</resource>
	</graph>
</rdf-mapping>
