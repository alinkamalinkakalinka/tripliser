<?xml version="1.0" encoding="UTF-8"?>
<rdf-mapping>
	<namespaces>
		<namespace prefix="foaf" url="http://xmlns.com/foaf/0.1/" />
	</namespaces>
	<graph>
		<resource query="//many/thing">
			<about prepend="http://bbc.co.uk/thing/" query="@id" />
			<properties>
				<property name="foaf:name" query="name" />
			</properties>
		</resource>
	</graph>
</rdf-mapping>
